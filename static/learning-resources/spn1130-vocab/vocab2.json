      "vocabulary": [
        {
          "es": "¿Cómo se llama (usted) / te llamas (tú)?",
          "en": "What is your name? (formal / informal)"
        },
        {
          "es": "Me llamo ...",
          "en": "My name is ..."
        },
        {
          "es": "¿Cómo está (usted)? / ¿Cómo estás (tú)?",
          "en": "How are you? (formal / informal)"
        },
        {
          "es": "¿Qué pasa? / ¿Qué hay de nuevo?",
          "en": "What's up? / What's new?"
        },
        {
          "es": "¿Qué tal?",
          "en": "How are you?"
        },
        {
          "es": "(No) Muy bien.",
          "en": "(Not) Very well."
        },
        {
          "es": "Regular. /Así así.",
          "en": "OK."
        },
        {
          "es": "¿De dónde es (usted)? / ¿De dónde eres (tú)?",
          "en": "Where are you from? (formal / informal)"
        },
        {
          "es": "Soy de ...",
          "en": "I'm from ..."
        },
        {
          "es": "¿Qué estudia (usted) / estudias (tú)?",
          "en": "What do you study? (formal / informal)"
        },
        {
          "es": "Estudio ...",
          "en": "I study ..."
        },
        {
          "es": "¿Cuál es tu especialización?",
          "en": "What's your major?"
        },
        {
          "es": "Mi especialización es ...",
          "en": "My major is ..."
        },
        {
          "es": "No sé.",
          "en": "I don't know."
        },
        {
          "es": "¿Y usted / tú?",
          "en": "And you? (formal / informal)"
        },
        {
          "es": "Gracias.",
          "en": "Thank you."
        },
        {
          "es": "Mucho gusto.",
          "en": "Pleased to meet you."
        },
        {
          "es": "Igualmente.",
          "en": "Likewise."
        }
      ]
